type Player{
    id: ID!
    name: String
    team: Team
}

enum Team{
    CSK,
    RCB,
    KKR,
    MI
}

type Query{
    findAll: [Player]
    getPlayer(id: ID): Player
}

type Mutation{
    create(name: String,team: Team): Player
    update(id:ID! name:String, team: Team): Player
    delete(id:ID!): Player
}

